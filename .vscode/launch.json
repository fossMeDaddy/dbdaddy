    "version": "0.2.0",
    "configurations": [
        {
            "name": "migrations-generate-dry-run",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["migrations", "generate", "--dry-run"]
        },
        {
            "name": "inspect-all",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": ["inspect", "--all"]
        }
    ]
}